   [<b>-mfloat-abi</b>=<var>abi</var>]
   [<b>-meabi</b>=<var>ver</var>]
   [<b>-mthumb</b>]
   [<b>-EB</b>|<b>-EL</b>]
   [<b>-mapcs-32</b>|<b>-mapcs-26</b>|<b>-mapcs-float</b>|
    <b>-mapcs-reentrant</b>]
   [<b>-mthumb-interwork</b>] [<b>-k</b>]

<em>Target Blackfin options:</em>
