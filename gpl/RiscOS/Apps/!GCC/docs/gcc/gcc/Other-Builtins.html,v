anhl-2129"></a><a name="index-ctanl-2130"></a><a name="index-dcgettext-2131"></a><a name="index-dgettext-2132"></a><a name="index-drem-2133"></a><a name="index-dremf-2134"></a><a name="index-dreml-2135"></a><a name="index-erf-2136"></a><a name="index-erfc-2137"></a><a name="index-erfcf-2138"></a><a name="index-erfcl-2139"></a><a name="index-erff-2140"></a><a name="index-erfl-2141"></a><a name="index-exit-2142"></a><a name="index-exp-2143"></a><a name="index-exp10-2144"></a><a name="index-exp10f-2145"></a><a name="index-exp10l-2146"></a><a name="index-exp2-2147"></a><a name="index-exp2f-2148"></a><a name="index-exp2l-2149"></a><a name="index-expf-2150"></a><a name="index-expl-2151"></a><a name="index-expm1-2152"></a><a name="index-expm1f-2153"></a><a name="index-expm1l-2154"></a><a name="index-fabs-2155"></a><a name="index-fabsf-2156"></a><a name="index-fabsl-2157"></a><a name="index-fdim-2158"></a><a name="index-fdimf-2159"></a><a name="index-fdiml-2160"></a><a name="index-ffs-2161"></a><a name="index-floor-2162"></a><a name="index-floorf-2163"></a><a name="index-floorl-2164"></a><a name="index-fma-2165"></a><a name="index-fmaf-2166"></a><a name="index-fmal-2167"></a><a name="index-fmax-2168"></a><a name="index-fmaxf-2169"></a><a name="index-fmaxl-2170"></a><a name="index-fmin-2171"></a><a name="index-fminf-2172"></a><a name="index-fminl-2173"></a><a name="index-fmod-2174"></a><a name="index-fmodf-2175"></a><a name="index-fmodl-2176"></a><a name="index-fprintf-2177"></a><a name="index-fprintf_005funlocked-2178"></a><a name="index-fputs-2179"></a><a name="index-fputs_005funlocked-2180"></a><a name="index-frexp-2181"></a><a name="index-frexpf-2182"></a><a name="index-frexpl-2183"></a><a name="index-fscanf-2184"></a><a name="index-gamma-2185"></a><a name="index-gammaf-2186"></a><a name="index-gammal-2187"></a><a name="index-gettext-2188"></a><a name="index-hypot-2189"></a><a name="index-hypotf-2190"></a><a name="index-hypotl-2191"></a><a name="index-ilogb-2192"></a><a 