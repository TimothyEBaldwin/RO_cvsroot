EXTRA_DIST = \
    .cvsignore \
    ChangeLog \
    Makefile.in \
    build_vms.com \
    config.h \
    config.h.in \
    dir.h \
    filesubr.c \
    filutils.c \
    filutils.h \
    getpass.c \
    getwd.c \
    misc.c \
    misc.h \
    ndir.c \
    ndir.h \
    pathnames.h \
    pc.c \
    pipe.c \
    pipe.h \
    piped_child.c \
    pwd.c \
    pwd.h \
    rcmd.c \
    readlink.c \
    rmdir.c \
    startserver.c \
    stat.c \
