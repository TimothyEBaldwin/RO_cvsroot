Version 0.39. Tagged as 'InetSetup-0_39'
@
text
@d15 1
a15 8
# Makefile for !InetSetup
#
# ***********************************
# ***    C h a n g e   L i s t    ***
# ***********************************
# Date       Name   Description
# ----       ----   -----------
# 08-Oct-96  KJB    Created
a17 3
#
# Program specific options:
#
d20 1
a20 2
RDIR       = Resources
LDIR       = ${RDIR}.${LOCALE}
d22 27
d50 1
a50 70
#
# Generic options:
#
MKDIR   = do mkdir -p
AS      = objasm
CC      = cc
CMHG    = cmhg
CP      = copy
LD      = link
RM      = remove
SQUEEZE = squeeze
WIPE    = x wipe

AFLAGS  = ${THROWBACK} -depend !Depend -stamp -quit
#CFLAGS  = ${THROWBACK} -c -depend !Depend -ffa -gflv -Wp ${INCLUDES} ${DFLAGS} -DOSLib_4_8 -DPlugIn
CFLAGS  = ${THROWBACK} -c -depend !Depend -ffa -gflv -Wp ${INCLUDES} ${DFLAGS} -DPlugIn
CPFLAGS = ~cfr~v
WFLAGS  = ~c~v

#
# Libraries
#
CLIB = CLIB:o.stubs
OSLIB = OSLib:o.OSLib
ILIB = TCPIPLibs:o.inetlib

#
# Include files
#
INCLUDES = -IOS:,C:,TCPIPLibs:

DFLAGS   = 

FILES = \
 ${RDIR}.!Boot\
 ${LDIR}.!Help\
 ${LDIR}.!Run\
 aif.!RunImage\
 ${LDIR}.!Sprites\
 ${LDIR}.!Sprites11\
 ${LDIR}.!Sprites22\
 ${LDIR}.Blanks.Routes\
 ${LDIR}.Blanks.SetUpNet\
 ${LDIR}.Blanks.User\
 ${RDIR}.CoSprite\
 ${RDIR}.CoSprite22\
 ${LDIR}.Messages\
 ${LDIR}.Res\
 ${LDIR}.Sprites\
 ${LDIR}.Sprites22

OBJS = AUN.o Detect.o Diagnose.o event.o FileUtils.o IfsDbox.o Load.o\
       Main.o ModUtils.o Save.o SmallDrag.o swis.o tbevent.o wimpevent.o\
       wimpmsg.o o.x

#
# Rule patterns
#
.c.o:;      ${CC} ${CFLAGS} -o $@@ $<
.s.o:;      ${AS} ${AFLAGS} -o $@@ $<

#
# Main rules:
#
# Application
#
all: ${FILES}
        @@echo ${COMPONENT}: Application built (Disc)

install: ${FILES}
d52 1
a52 9
        ${MKDIR} ${INSTAPP}.AutoSense
        ${CP} ${RDIR}.!Boot      ${INSTAPP}.!Boot      ${CPFLAGS}
        ${CP} ${LDIR}.!Help      ${INSTAPP}.!Help      ${CPFLAGS}
        ${CP} ${LDIR}.!Run       ${INSTAPP}.!Run       ${CPFLAGS}
        ${CP} aif.!RunImage      ${INSTAPP}.!RunImage  ${CPFLAGS}
        ${CP} ${LDIR}.!Sprites   ${INSTAPP}.!Sprites   ${CPFLAGS}
        ${CP} ${LDIR}.!Sprites11 ${INSTAPP}.!Sprites11 ${CPFLAGS}
        ${CP} ${LDIR}.!Sprites22 ${INSTAPP}.!Sprites22 ${CPFLAGS}
        ${CP} ${LDIR}.EtherUSB   ${INSTAPP}.AutoSense.EtherUSB   ${CPFLAGS}
a53 1
        ${CP} ${LDIR}.Blanks.SetUpNet ${INSTAPP}.Blanks.SetUpNet ${CPFLAGS}
a54 25
        ${CP} ${RDIR}.CoSprite   ${INSTAPP}.CoSprite   ${CPFLAGS}
        ${CP} ${RDIR}.CoSprite22 ${INSTAPP}.CoSprite22 ${CPFLAGS}
        ${CP} ${LDIR}.Messages   ${INSTAPP}.Messages   ${CPFLAGS}
        ${CP} ${LDIR}.Res        ${INSTAPP}.Res        ${CPFLAGS}
        ${CP} ${LDIR}.Sprites    ${INSTAPP}.Sprites    ${CPFLAGS}
        ${CP} ${LDIR}.Sprites22  ${INSTAPP}.Sprites22  ${CPFLAGS}
        Access ${INSTAPP} /r
        Access ${INSTAPP}.* wr/r
        Access ${INSTAPP}.Blanks.* wr/r
        @@echo ${COMPONENT}: Application installed (Disc)

clean:
        ${WIPE} o.* ${WFLAGS}
        ${WIPE} aif ${WFLAGS}
        @@echo ${COMPONENT}: cleaned

o.x: c.x

#
# Static dependencies:
#
aif.!RunImage: ${OBJS} ${CLIB} ${OSLIB} ${ILIB}
        ${MKDIR} aif
        ${LD} -o $@@ ${OBJS} ${CLIB} ${OSLIB} ${ILIB} ${LDFLAGS}
        ${SQUEEZE} $@@
d56 2
a58 1
#---------------------------------------------------------------------------
@


1.9
log
@  Added an AutoSense script for EtherUSB
Detail:
  Deduces how many USB devices are connected by parsing the output of
  *EJInfo, and dynamically sets the appropriate system variables to
  configure the interfaces window of InetSetup. Note that although the
  AutoSense script for EtherK is held within the EtherK sources, we don't
  currently distribute the sources to EtherUSB, so this seems a reasonable
  second choice of where to install it from.
Admin:
  Tested with 0, 1 or 2 devices connected to a beagleboard.

Version 0.38. Tagged as 'InetSetup-0_38'
@
