        ${CP} ${LDIR}.Blanks.SetUpNet ${INSTDIR}.Blanks.SetUpNet ${CPFLAGS}
        ${CP} ${LDIR}.Blanks.User     ${INSTDIR}.Blanks.User     ${CPFLAGS}
