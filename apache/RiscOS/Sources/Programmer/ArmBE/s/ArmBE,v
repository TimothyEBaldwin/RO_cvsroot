; ******************************************************************************
;
; do_osbyte_x0_y0
; => R0 = OS_Byte reason code
; <= Return values as though OS_Byte,R0,0,0 was performed
;
do_osbyte_x0_y0
        Push    "R14"
        MOV     R1,#0
        MOV     R2,#0
        SWI     OS_Byte
        Pull    "PC"

; ******************************************************************************
;
; do_osbyte_y0
; => R0 = OS_Byte reason code
;    R1 = OS_Byte parameter
; <= Return values as though OS_Byte,R0,R1,0 was performed
;
do_osbyte_y0
        Push    "R14"
        MOV     R2,#0
        SWI     OS_Byte
        Pull    "PC"

