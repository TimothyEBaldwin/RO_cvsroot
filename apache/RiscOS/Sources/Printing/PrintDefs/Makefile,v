text
@a16 28
# ***********************************
# ***    C h a n g e   L i s t    ***
# ***********************************
# Date       Name         Description
# ----       ----         -----------
# 19-Jul-96  SMC          Created
# 06-May-99  SBF          Included additional printer defintions
# 15-Aug-00  SBF          Palette files now copied using wildcard
# 01-Mar-03  RPS          Split to allow STB oriented and desktop oriented output
# 12-May-04  JWB	  use chmod rather than Attr for mode setting
#
# Generic options:
#
MKDIR   = mkdir -p
CP      = copy
RM      = remove
WIPE    = x wipe
COMPRESS = squash
ATTR    = chmod
#ATTR    = -Attr

CPFLAGS = ~cfr~v
WFLAGS  = ~c~v

#
# Export Paths for Messages module
#
RESDIR = <resource$dir>.Resources.${COMPONENT}
d25 3
d98 1
a98 1
        ${MKDIR} ${APP}
d149 13
a161 11
install: ${DESKTOPFILES} desktopdirs
         ${CP} ${RDIR}.!Boot                 ${APP}.!Boot           ${CPFLAGS}
         ${CP} ${RDIR}.!Run                  ${APP}.!Run            ${CPFLAGS}
         ${CP} ${RDIR}.!Sprites              ${APP}.!Sprites        ${CPFLAGS}
         ${CP} ${RDIR}.!Sprites11            ${APP}.!Sprites11      ${CPFLAGS}
         ${CP} ${RDIR}.!Sprites22            ${APP}.!Sprites22      ${CPFLAGS}
         ${CP} ${RDIR}.USBMap                ${APP}.USBMap          ${CPFLAGS}
         ${CP} ${RDIR}.<Locale>.!Help        ${APP}.!Help           ${CPFLAGS}
         ${CP} ${RDIR}.<Locale>.NonSTB.*     ${APP}.Printers.*      ${CPFLAGS}
         ${CP} ${RDIR}.NonSTB.*              ${APP}.Printers.*      ${CPFLAGS}
         ${ATTR} -f  -R 444                  ${APP}
