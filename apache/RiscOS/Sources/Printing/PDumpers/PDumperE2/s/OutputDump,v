        MOV     R1,#0
        STR     R1,[SP]
        LDRB    R1,[R0,#0]      ;At least one byte (see above)
        STRB    R1,[SP,#0]
        CMP     R3,#2
        LDRGEB  R1,[R0,#1]
        STRGEB  R1,[SP,#1]
