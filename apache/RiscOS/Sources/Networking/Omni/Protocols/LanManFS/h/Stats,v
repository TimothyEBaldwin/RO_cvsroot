#define STA_MAXSTATS 29

extern int Stat_StatTable [STA_MAXSTATS];

#define STAT(a)  (Stat_StatTable[(a)]++)
#define Stat_IncBy(a,n)  (Stat_StatTable[(a)]+=(n))
