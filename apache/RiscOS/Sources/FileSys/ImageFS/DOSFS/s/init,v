        IMPORT  |_clib_initialisemodule|

        EXPORT  |_dosfs_initialisemodule|

        GET     Hdr:ListOpts
        GET     Hdr:Macros
        GET     Hdr:System
        GET     Hdr:MsgTrans
        GET     Hdr:FSNumbers
        GET     Hdr:NewErrors
        
        AREA    |C$$Code|, CODE, READONLY

|_dosfs_initialisemodule|
        ADR     r7, |_dosfs_initialisemodule|
        LDR     r8, |_dosfs_initialisemodule_adcon|
        CMP     r7, r8
        BEQ     |_clib_initialisemodule|
        ADR     r0, module_ramload_error
        MOV     r1, #0
        MOV     r2, #0
        ADR     r4, titlestr
        MOV     r5, #0
        MOV     r6, #0
        MOV     r7, #0
        SWI     XMessageTrans_ErrorLookup
        Pull    "r7-r11, pc"

|_dosfs_initialisemodule_adcon|
        DCD     |_dosfs_initialisemodule|

module_ramload_error
        DCD     ErrorBase_CSharedLibrary + 10 ; See 'RISC_OSLib/modulewrap.s'
        DCB     "RAMLoadC", 0
        ALIGN

titlestr
        DCB     "DOSFS", 0
        ALIGN

        END
@


4.2
log
@Fixed to build RAM version correctly without the "Cannot be RAM loaded" check.
Modified "Cannot be RAM loaded" check to work correctly with recent versions
of CMHG - was branching through zero.

Version 0.65. Tagged as 'DOSFS-0_65'
@
text
@a14 16
r0              RN      0
r1              RN      1
r2              RN      2
r3              RN      3
r4              RN      4
r5              RN      5
r6              RN      6
r7              RN      7
r8              RN      8
r9              RN      9
r10             RN      10
r11             RN      11
r12             RN      12
sp              RN      13
lr              RN      14
pc              RN      15
d16 1
a16 2
XMessageTrans_ErrorLookup EQU &61506
MessageTrans_ErrorLookup  EQU &41506
d18 1
a18 1
                IMPORT  |_clib_initialisemodule|
d20 8
a27 3
                EXPORT  |_dosfs_initialisemodule|

                AREA    |C$$Code|, CODE, READONLY
d30 13
a42 13
                ADR     r7, |_dosfs_initialisemodule|
                LDR     r8, |_dosfs_initialisemodule_adcon|
                CMP     r7, r8
                BEQ     |_clib_initialisemodule|
                ADR     r0, module_ramload_error
                MOV     r1, #0
                MOV     r2, #0
                ADR     r4, titlestr
                MOV     r5, #0
                MOV     r6, #0
                MOV     r7, #0
                SWI     XMessageTrans_ErrorLookup
                LDMIA   sp!, {r7-r11, pc}
d45 1
a45 1
                DCD     |_dosfs_initialisemodule|
d48 7
a54 6
                DCD     &800e0a
                DCB     "RAMLoadC", 0
                ALIGN

titlestr        DCB     "DOSFS", 0
                ALIGN
d56 1
a56 1
                END
@


