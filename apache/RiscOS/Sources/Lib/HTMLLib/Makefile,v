1.1.2.1
log
@Initial revision
@
text
@a0 230
# Copyright 1997 Acorn Computers Ltd
#
# Licensed under the Apache License, Version 2.0 (the "License");
# you may not use this file except in compliance with the License.
# You may obtain a copy of the License at
#
#     http://www.apache.org/licenses/LICENSE-2.0
#
# Unless required by applicable law or agreed to in writing, software
# distributed under the License is distributed on an "AS IS" BASIS,
# WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.
# See the License for the specific language governing permissions and
# limitations under the License.
#
# Makefile for HTMLLib
#
# ***********************************
# ***    C h a n g e   L i s t    ***
# ***********************************
# Date       Name         Description
# ----       ----         -----------
# 12-Dec-96  KJB          Created.
#

#
# Paths
#
EXP_HDR = <export$dir>
LIBDIR = <Lib$Dir>

#
# Generic options:
#
MKDIR   = cdir
AS      = objasm
CP      = copy
CC      = cc
CM      = cmhg
RM      = remove
LD      = link
LB      = libfile
WIPE    = wipe

CCFLAGS = -c -depend !Depend -fah -g -throwback ${INCLUDES} ${DFLAGS}
ASFLAGS = -depend !Depend -Stamp -quit
CPFLAGS = ~cfr~v
LBFLAGS = -c -o
WFLAGS  = ~CFR~V

#
# Libraries
#
CLIB      = CLIB:o.stubs
RLIB      = RISCOSLIB:o.risc_oslib
RSTUBS    = RISCOSLIB:o.rstubs
ROMSTUBS  = RISCOSLIB:o.romstubs
ROMCSTUBS = RISCOSLIB:o.romcstubs
ABSSYM    = RISC_OSLib:o.AbsSym

#
# Include files
#
INCLUDES = -IC:
DFLAGS    = -DLIBRARY -DNDEBUG

#
# Program specific options:
#
COMPONENT = HTMLLib
SOURCE    = ...
TARGET    = ${COMPONENT}
EXPORTS   = ${EXP_HDR}.${COMPONENT}

OBJS =\
 o.display\
 o.malloc\
 o.rules\
 o.veneers\
 o.new_stream\
 o.anchor_inf\
 o.HTTPSuport\
 o.font

#
# Rule patterns
#
.SUFFIXES: .o .s .h .cmhg .c
.c.o:;     ${CC} ${CCFLAGS} -o $@@ $<
.s.o:;     ${AS} ${ASFLAGS} -o $@@ $<
.cmhg.o:;  ${CM} ${CMFLAGS} -o $@@ $<
.cmhg.h:;  ${CM} ${CMFLAGS} -d $@@ $<

#
# build a the library:
#
all: ${COMPONENT}

#
# build a test program:
#
html_riscos2: ${COMPONENT} o.main o.svcprint o.irqs ${CLIB}
	${LD} -aif -o html_riscos2 ${COMPONENT} o.main o.svcprint o.irqs ${CLIB}

#
# RISC OS ROM build rules:
#
rom: ${TARGET}
        @@echo ${COMPONENT}: rom module built

export: export_${PHASE}

install_rom: ${TARGET}
        ${CP} ${TARGET} ${INSTDIR}.${COMPONENT} ${CPFLAGS}
        @@echo ${COMPONENT}: rom module installed

clean:
        ${WIPE} o.* ${WFLAGS}
        ${RM} ${TARGET}
        ${RM} html_riscos2
        @@echo ${COMPONENT}: cleaned

export_hdrs: h.html2_ext h.api h.struct h.tags dirs
	Copy h.html2_ext    HTMLLib:h.html2_ext      ~CFLNR~V
	Copy h.api          HTMLLib:h.HTMLLib        ~CFLNR~V
	Copy h.struct       HTMLLib:h.struct         ~CFLNR~V
	Copy h.tags         HTMLLib:h.tags           ~CFLNR~V
        @@echo ${COMPONENT}: export complete (hdrs)

export_libs: ${TARGET} dirs
	Copy ${TARGET}      HTMLLib:o.${TARGET}      ~CFLNR~V
        @@echo ${COMPONENT}: export complete (libs)

dirs:
        ${MKDIR} ${LIBDIR}
        ${MKDIR} ${LIBDIR}.HTMLLib
        ${MKDIR} ${LIBDIR}.HTMLLib.h
        ${MKDIR} ${LIBDIR}.HTMLLib.o

#
# Final link
#
${COMPONENT}: ${OBJS}
        ${LB} ${LBFLAGS} ${COMPONENT} ${OBJS}
        ${CP} @@.${COMPONENT} @@.o.${COMPONENT} ${CPFLAGS}

${EXP_HDR}.${COMPONENT}: hdr.${COMPONENT}
        ${CP} hdr.${COMPONENT} $@@ ${CPFLAGS}

# Dynamic dependencies:
o.malloc:	c.malloc
o.rules:	c.rules
o.rules:	h.html2_ext
o.rules:	h.internal
o.rules:	h.tags
o.rules:	h.struct
o.rules:	C:h.ctype
o.rules:	h.html2
o.rules:	h.tags
o.rules:	h.rules
o.rules:	h.tags
o.rules:	h.html2_ext
o.rules:	h.struct
o.rules:	C:h.string
o.rules:	C:h.ctype
o.veneers:	c.veneers
o.veneers:	h.api
o.veneers:	h.html2_ext
o.veneers:	h.tags
o.veneers:	h.struct
o.veneers:	h.internal
o.veneers:	h.tags
