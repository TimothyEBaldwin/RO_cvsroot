# Makefile for Toolbox
#
# ***********************************
# ***    C h a n g e   L i s t    ***
# ***********************************
# Date       Name   Description
# ----       ----   -----------
# 06-Jan-94  AMcC   Created
# 04-Feb-98  SNB    Updated to allow RAM builds
# 11-Jun-98  SNB    Updated to allow debug builds and add -p option to cmhg.
#

.SUFFIXES: .c .o .od .s

d19 1
a19 118
COMPONENT  = Toolbox
ROM_MODULE = aof.${COMPONENT}
RAM_MODULE = rm.${COMPONENT}
DBG_MODULE = rm.D${COMPONENT}

#
# Export Paths for Messages module
#
RESDIR = <resource$dir>.Resources2.${COMPONENT}

#
# Generic options:
#
AS      = objasm
CC      = cc
CMHG    = cmhg
CP      = copy
LD      = link
TLD     = tlink
MKDIR   = cdir
MODSQZ  = modsqz
RESGEN	= resgen
RM      = remove
WIPE    = -wipe

CPFLAGS = ~cfr~v
WFLAGS  = ~c~v

AFLAGS   = -depend !Depend -Stamp -quit -CloseExec
CFLAGS   = -c -depend !Depend ${INCLUDES} -zM -ffah -zps1 ${DFLAGS}
DFLAGS   = -D${SYSTEM}
ROMFLAGS = -DROM
INCLUDES = -I<Lib$Dir>.tboxlibint.,<Lib$Dir>.tboxlibs:,C:

#
# Libraries
#
ANSILIB   = CLib:o.ansilib
CLIB      = CLIB:o.stubs
RLIB      = RISCOSLIB:o.risc_oslib
ROMCSTUBS = RISCOSLIB:o.romcstubs
ABSSYM    = RISC_OSLib:o.abssym
TBOXLIB   = <Lib$Dir>.tboxlibint.o.tboxlibm

OBJS =\
 o.Modhdr_NoD\
 o.base\
 o.callback\
 o.event\
 o.filters\
 o.filter_ven\
 o.globals\
 o.main\
 o.memory\
 o.memswis\
 o.object\
 o.resf\
 o.task

RAM_OBJS =\
 o.Modhdr_NoD\
 o.base\
 o.callback\
 o.event\
 o.filters\
 o.filter_ven\
 o.globals\
 o.mainRAM\
 o.memory\
 o.memswis\
 o.object\
 o.resf\
 o.task

DBG_OBJS =\
 o.Modhdr\
 o.base\
 o.callback\
 od.event\
 od.filters\
 o.filter_ven\
 od.globals\
 od.mainRAM\
 od.memory\
 o.memswis\
 od.object\
 od.resf\
 od.task

RESF=o.msgs

#
# Rule patterns
#
.c.o:;    ${CC} ${CFLAGS} ${ROMFLAGS} -o $@@ $<
.c.od:;    ${CC} ${CFLAGS} ${ROMFLAGS} -Ddebugging=1 -o $@@ $<
.cmhg.o:; ${CMHG} -depend !Depend -p -o $@@ $<
.s.o:;    ${AS} ${AFLAGS} -o $@@ $<

#
# Main rules:
#
#
all: ${RAM_MODULE}
        @@echo ${COMPONENT}: Module built (RAM)

rom: ${ROM_MODULE}
        @@echo ${COMPONENT}: Module built (ROM)

install_rom: ${ROM_MODULE}
        ${CP} ${ROM_MODULE} ${INSTDIR}.${COMPONENT} ${CPFLAGS}
        @@echo ${COMPONENT}: Module installed (ROM)

resources: LocalRes:Messages
        ${MKDIR} ${RESDIR}
	TokenCheck LocalRes:Messages
        ${CP} LocalRes:Messages ${RESDIR}.Messages ${CPFLAGS}
        @@echo ${COMPONENT}: resource files copied to Messages module
d21 2
a22 6
clean:
        ${WIPE} o.* ${WFLAGS}
        ${WIPE} aof ${WFLAGS}
        ${WIPE} rm ${WFLAGS}
        ${WIPE} linked ${WFLAGS}
        @@echo ${COMPONENT}: cleaned
d24 1
a24 2
trace: ${DBG_MODULE}
       @@echo ${COMPONENT}: Debug module build (RAM)
d26 1
d28 2
a29 3
${ROM_MODULE}: ${OBJS} ${TBOXLIB} ${ROMCSTUBS}
        ${MKDIR} aof
        ${LD} -o $@@ -aof ${OBJS} ${TBOXLIB} ${ROMCSTUBS}
d31 4
a34 6
# final link for ROM Image (using given base address)
rom_link:
        ${MKDIR} linked
        ${LD} -o linked.${COMPONENT} -rmf -base ${ADDRESS} ${ROM_MODULE} ${ABSSYM}
        ${CP} linked.${COMPONENT} ${LINKDIR}.${COMPONENT} ${CPFLAGS}
        @@echo ${COMPONENT}: rom_link complete
d37 1
a37 1
# Explicit rules
d39 2
a40 2
${RESF}: LocalRes:Messages
	resgen messages_file ${RESF} LocalRes:Messages Resources.${COMPONENT}.Messages
d42 2
a43 16
${RAM_MODULE}: ${RAM_OBJS} ${TBOXLIB} ${CLIB} ${RESF} ${CLIB}
	${mkdir} rm
        ${LD} -o $@@ -module ${RAM_OBJS} ${TBOXLIB} ${RESF} ${CLIB}
        ${MODSQZ} $@@
        Access $@@ RW/R

${DBG_MODULE}: ${DBG_OBJS} ${TBOXLIB} ${CLIB} ${RESF} ${CLIB}
	${mkdir} rm
        ${LD} -o $@@ -module ${DBG_OBJS} ${TBOXLIB} ${RESF} ${CLIB} <Lib$Dir>.tboxlibint.o.debuglibm
        Access $@@ RW/R

o.mainRAM: main.c
	$(CC) ${CFLAGS} -o $@@ main.c

od.mainRAM: main.c
	$(CC) ${CFLAGS} -Ddebugging=1 -o $@@ main.c
