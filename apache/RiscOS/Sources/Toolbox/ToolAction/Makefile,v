.c.o:;      ${CC} ${CFLAGS} -o $@@ $< -DROM=0
.c.oz:;     ${CC} ${CFLAGS} -o $@@ $< -DROM=1
.cmhg.o:;   ${CMHG} -p -o $@@ $< h.$* -DROM=0
