# Makefile for Window
d17 1
a17 8
# ***********************************
# ***    C h a n g e   L i s t    ***
# ***********************************
# Date       Name   Description
# ----       ----   -----------
# 06-Jan-94  AMcC   Created
# 04-Feb-98  SNB    Updated to allow RAM builds
# 08-Jun-98  SNB    Added -p flag to cmhg invocation
d19 20
a38 1
.SUFFIXES: .c .o .od .s
d40 6
a45 1
VPATH = @@ gadgets
d48 1
a48 1
# Component specific options:
d50 1
a50 177
COMPONENT  = Window
ROM_MODULE = aof.${COMPONENT}
RAM_MODULE = rm.${COMPONENT}
DBG_MODULE = rm.D${COMPONENT}

#
# Export Paths for Messages module
#
RESDIR = <resource$dir>.Resources2.${COMPONENT}

#
# Generic options:
#
AS      = objasm
CC      = cc
CMHG    = cmhg
CP      = copy
LD      = link
TLD	= tlink
MSQZ	= modsqz
MKDIR   = cdir
RM      = remove
WIPE    = -wipe
CD      = dir
MAKE    = amu
TOUCH   = create

CPFLAGS = ~cfr~v
WFLAGS  = ~c~v

AFLAGS   = -depend !Depend -Stamp -quit -CloseExec
CFLAGS   = -c -depend !Depend ${INCLUDES} -zM -ffah -Wc -zps1 ${DFLAGS}
DFLAGS   = -D${SYSTEM} ${STYLE}
ROMFLAGS = -DROM
INCLUDES = -IC:tboxlibint.,C:tboxlibs,C:

#
# Libraries
#
ANSILIB   = CLib:o.ansilib
CLIB      = CLIB:o.stubs
RLIB      = RISCOSLIB:o.risc_oslib
ROMCSTUBS = RISCOSLIB:o.romcstubs
ABSSYM    = RISC_OSLib:o.c_abssym
TBOXLIB   = C:tboxlibint.o.tboxlibm
WIMPLIB   = C:tboxlibs.o.wimplib

OBJS =\
 o.ModNoDbg\
 o.actbut\
 o.adjuster\
 o.button\
 o.create\
 o.delete\
 o.display\
 o.draggable\
 o.events\
 o.gadgets\
 o.getstate\
 o.globals\
 o.hide\
 o.label\
 o.labelbox\
 o.main\
 o.miscop\
 o.numrange\
 o.optbut\
 o.popupmenu\
 o.radiobut\
 o.show\
 o.simple\
 o.slider\
 o.slider_ven\
 o.stringset\
 o.task\
 o.toolbar\
 o.toolbox\
 o.utils\
 o.writable

RAM_OBJS =\
 o.ModNoDbg\
 o.actbut\
 o.adjuster\
 o.button\
 o.create\
 o.delete\
 o.display\
 o.draggable\
 o.events\
 o.gadgets\
