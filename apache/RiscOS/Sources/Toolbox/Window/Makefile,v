RESF=o.msgs

#
# Rule patterns
#
.c.o:;    ${CC} ${CFLAGS} ${ROMFLAGS} -o $@@ $<
.c.od:;    ${CC} ${CFLAGS} ${ROMFLAGS} -Ddebugging=1 -o $@@ $<
.cmhg.o:; ${CMHG} -p -o $@@ $<
.s.o:;    ${AS} ${AFLAGS} -o $@@ $<

#
# Main rules:
#
#
all: ${RAM_MODULE} o._dirs
        @@echo ${COMPONENT}: Module built (RAM)

rom: ${ROM_MODULE} o._dirs
        @@echo ${COMPONENT}: Module built (ROM)

install_rom: ${ROM_MODULE} o._dirs
        ${CP} ${ROM_MODULE} ${INSTDIR}.${COMPONENT} ${CPFLAGS}
        @@echo ${COMPONENT}: Module installed (ROM)

resources: LocalRes:Messages
        ${MKDIR} ${RESDIR}
	TokenCheck LocalRes:Messages
        ${CP} LocalRes:Messages ${RESDIR}.Messages ${CPFLAGS}
d53 1
a53 8
trace: ${DBG_MODULE}
       @@echo ${COMPONENT}: Debug module build (RAM)

clean:
        ${WIPE} o.* ${WFLAGS}
        ${WIPE} aof ${WFLAGS}
        ${WIPE} rm ${WFLAGS}
        ${WIPE} linked ${WFLAGS}
a54 39

${ROM_MODULE}: ${OBJS} ${TBOXLIB} ${WIMPLIB} ${ROMCSTUBS}
        ${MKDIR} aof
        ${LD} -o $@@ -aof ${OBJS} ${TBOXLIB} ${WIMPLIB} ${ROMCSTUBS}

# final link for ROM Image (using given base address)
rom_link:
        ${MKDIR} linked
        ${LD} -o linked.${COMPONENT} -rmf -base ${ADDRESS} ${ROM_MODULE} ${ABSSYM}
        ${CP} linked.${COMPONENT} ${LINKDIR}.${COMPONENT} ${CPFLAGS}
        @@echo ${COMPONENT}: rom_link complete

${WIMPLIB}:
        ${CD} ^.wimplib
        ${MAKE} @@.wimplib
        back

o._dirs:
	${MKDIR} o
	${TOUCH} $@@

${RESF}: LocalRes:Messages
	resgen messages_file ${RESF} LocalRes:Messages Resources.${COMPONENT}.Messages

${RAM_MODULE}: ${RAM_OBJS} ${TBOXLIB} ${WIMPLIB} ${RESF} ${CLIB}
	${mkdir} rm
        ${LD} -o $@@ -module ${RAM_OBJS} ${TBOXLIB} ${WIMPLIB} ${RESF} ${CLIB}
        ${MSQZ} $@@
        Access $@@ RW/R

${DBG_MODULE}: ${DBG_OBJS} ${TBOXLIB} ${WIMPLIB} ${RESF} ${CLIB}
	${mkdir} rm
        ${LD} -o $@@ -module ${DBG_OBJS} ${TBOXLIB} ${WIMPLIB} <Lib$Dir>.tboxlibint.o.debuglibm <syslog$dir>.c-veneer.o.syslog ${RESF} ${CLIB}
        Access $@@ RW/R

o.mainRAM: main.c
	$(CC) ${CFLAGS} -o $@@ main.c
od.mainRAM: main.c
	$(CC) ${CFLAGS} -Ddebugging=1 -o $@@ main.c
@


