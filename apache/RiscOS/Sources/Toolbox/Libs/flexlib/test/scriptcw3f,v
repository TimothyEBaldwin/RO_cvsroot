@# Simple script for malloc/flex tester


repeat 1000

repeat 4
  echo iteration %0
  malloc 4097
  flex   1k 2k 3k 4k 5k 6k 7k 8k
  flex   4098
  ffree  2k 4k
  ffree  1k 5k
  ffree  3k 8k 7k
  ffree 4098 6k
  flex 4321
  flexextend 4321 7653
  flexextend 7653 2345
  flexmidextend 2345 999 100
  ffree 2445
loop
repeat 8
  echo iteration %0
  flex   7k 8k 9k 31k 32k 33k
  ffree  31k 7k
  flex 4321
  flexextend 4321 7653
  flexextend 7653 2345
  flexmidextend 2345 999 100
  ffree 2445
  ffree  32k 8k
  ffree  33k 9k
loop

loop@
