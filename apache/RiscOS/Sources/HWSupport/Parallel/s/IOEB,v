		BNE	%FT70
		SUBS	r4, r4, #1
		BGT	%BT65
		B	nibble_error
d1181 3
a1183 3
		LDRB	r3, [r2]				; ready to read the second nibble, similar cycle as above
		ORR	r3, r3, #pp_ctrl_autofd			; pull the autofd line down
		STRB	r3, [r2]
d1185 1
