d1197 9
a1205 9
		LDRB	r4, [r1]
		TST	r4, #pp_stat_notbusy			; see if bit seven is set
		ORREQ	r0, r0, #&80
		AND	r3, r4, #pp_stat_pe :OR: pp_stat_slct :OR: pp_stat_error ; find bits 2, 1 & 0, masked as 5 , 4 & 3
		ORR	r0, r0, r3, LSL #1			; set the top nibble

