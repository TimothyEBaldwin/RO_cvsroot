exit_with_message

		ENTRY 		"R1-R6"
		Debug		j, "exit with message"
		
		BL		OpenMessages
		ADDVS		r6, r0, #4
		BVS		send_exit		;if it went wrong, use the error as the message

		ADRL		r0, MessagesWorkspace
		MOV		r1, r6
		ADRL		r2, localerrorblock
		MOV		r3, #?localerrorblock
		SWI		XMessageTrans_Lookup
		MOVVC		r6, r2
		ADDVS		r6, r0, #4
				
		BL		CloseMessages
