.SUFFIXES: .oz .od .i .asm
.c.o:;      ${CC} ${CFLAGS} -ff -o $@@ $<
.c.oz:;     ${CC} ${CFLAGS} -ff -zM -zps1 -o $@@ $<
.c.od:;     ${CC} ${CFLAGS} -DTRACE=1 -o $@@ $<
.asm.o:;    ${AS} ${AFLAGS} $< $@@
.c.i:;      ${CC} -E ${CFLAGS} $< > $@@
.c.s:;      ${CC} ${CFLAGS} -s $<
