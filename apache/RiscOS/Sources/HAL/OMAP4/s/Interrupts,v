HAL_IRQClear    ROUT
        CMP     a1, #32
        SUBHS   a1, a1, #32     ; Map to GIC interrupt number
a203 6
        CMP     a1, #16         ; SGI?
        BHS     %FT10
        MRC     p15, 0, a2, c0, c0, 5
        TST     a2, #1
        ADDEQ   a1, a1, #1<<10  ; Assume it came from the other CPU
