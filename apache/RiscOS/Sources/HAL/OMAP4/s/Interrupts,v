	ADD	a1, a1, #MPU_INTC_DIST
	MOV	a2, #0		; disable ICDDCR
	STR	a2, [a1, #GIC_DIST_CTRL]

	; 2. set all global interrupts to be level triggered, active low
	ADD	a3, a1, #GIC_DIST_CONFIG
	ADD	a3, a3, #8
	MOV	a4, #INTERRUPT_MAX
	MOV	ip, #32
