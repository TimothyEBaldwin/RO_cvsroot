  [ {FALSE}
        ; Linux does not do this
        ; set IO pins and O pins as output: GPIO_85, GPIO_88 .. GPIO_95
        LDR     a1, L4_GPIO3_Log
        LDR     a2, [a1, #GPIO_OE]
        BIC     a2, a2, #(0xFF << (88 - 64))
        BIC     a2, a2, #(1 << (85 - 64))
        STR     a2, [a1, #GPIO_OE]
