;
; Stub functions for unsupported/unimplemented HAL features

        GET     Hdr:ListOpts
        GET     Hdr:Macros
        GET     Hdr:System
        GET     Hdr:Machine.<Machine>
        GET     Hdr:ImageSize.<ImageSize>

        GET     Hdr:Proc
        GET     Hdr:OSEntries

        GET     hdr.omap4430
        GET     hdr.StaticWS
        GET     hdr.UART

        AREA    |Asm$$Code|, CODE, READONLY, PIC

        EXPORT  HAL_ATAControllerInfo

HAL_ATAControllerInfo
        MOV     a1, #0
        MOV     pc, lr

        END
@


