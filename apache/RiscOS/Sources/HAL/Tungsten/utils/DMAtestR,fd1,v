DIM block% 8191
block% = (block%+4095) AND NOT 4095
DIM dmablk% 63
dmablk% = (dmablk%+31) AND NOT 31
PRINT ~dmablk%,~block%
DIM blk% 23
blk%!4 = dmablk%
blk%!16 = block%
SYS"OS_Memory",&A200,blk%,2
dmablkp% = blk%!8
blockp% = blk%!20
PRINT ~dmablkp%,~blockp%
dmablk%!0=0
dmablk%!4=&08000000
dmablk%!8=&00000000
dmablk%!12=blockp%
dmablk%!16=4096
dmablk%!20=&E
PROCpokew(PMMR%+&400,0)
PROCpokew(PMMR%+&410,dmablkp%)
PROCpokew(PMMR%+&400,1)
SYS"OS_Memory",&C600,blk%,2
@
