#ifndef DCIERROR_H
#define DCIERROR_H

#include "swis.h"
#include "sys/errno.h"


/*
 * dcierror converts unix errno numbers and DCI error numbers to _kernel_oserrors
 */


_kernel_oserror* dcierror(int errornum);

_kernel_oserror* dcierror_init(void);
void             dcierror_final(void);

#endif
