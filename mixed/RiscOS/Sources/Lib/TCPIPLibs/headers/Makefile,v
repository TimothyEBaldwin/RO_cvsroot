# Makefile for include

# ***********************************
# ***    C h a n g e   L i s t    ***
# ***********************************
# Date          Name         Description
# ----          ----         -----------
# 07-Feb-96     KJB          Created.
# 06-May-98     SNB          Copy ioctl_comp as ioctl_compat on export (long name)

LIBDIR = <Lib$Dir>

#
# Generic options:
#
MKDIR   = cdir
WIPE    = x wipe
CP      = copy
RM	= remove

WFLAGS  = ~c~v
CPFLAGS = ~cflnr~v

#
# Program specific options:
#
COMPONENT = include

HDRS = \
  arpa.h.ftp \
  arpa.h.inet \
  arpa.h.nameser \
  arpa.h.telnet \
  arpa.h.tftp \
  h.delay \
  h.errno \
  h.int_hndlr \
  h.ioc \
  h.netdb \
  h.pwd \
  h.riscos \
  h.sgtty \
  h.svcdebug \
  h.utmp \
  h.xcb \
  net.h.if \
  net.h.if_arp \
  net.h.netisr \
  net.h.raw_cb \
  net.h.route \
  netccitt.h.x25 \
  netinet.h.icmp_var \
  netinet.h.if_ether \
  netinet.h.in \
  netinet.h.in_pcb \
  netinet.h.in_systm \
  netinet.h.in_var \
  netinet.h.ip \
  netinet.h.ip_icmp \
  netinet.h.ip_var \
  netinet.h.tcp \
  netinet.h.tcpip \
  netinet.h.tcp_debug \
  netinet.h.tcp_fsm \
  netinet.h.tcp_seq \
  netinet.h.tcp_timer \
  netinet.h.tcp_var \
  netinet.h.udp \
  netinet.h.udp_var \
  netiso.h.iso \
  netiso.h.iso_var \
  netns.h.ns \
  netns.h.ns_if \
  protocols.h.routed \
  rpc.h.auth \
  rpc.h.auth_unix \
  rpc.h.clnt \
  rpc.h.netdb \
  rpc.h.pmap_clnt \
  rpc.h.pmap_prot \
  rpc.h.pmap_rmt \
  rpc.h.rpc \
  rpc.h.rpc_msg \
  rpc.h.svc \
  rpc.h.svc_auth \
  rpc.h.types \
  rpc.h.xdr \
  rpcsvc.h.bootparam \
  sys.arm.h.fp \
  sys.h.acct \
  sys.h.buf \
  sys.h.callout \
  sys.h.clist \
  sys.h.conf \
  sys.h.dcistructs \
  sys.h.dir \
  sys.h.domain \
  sys.h.errno \
  sys.h.fcntl \
  sys.h.file \
  sys.h.ioctl \
  sys.h.ipc \
  sys.h.mbuf \
