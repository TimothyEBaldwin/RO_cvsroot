@/* fileswch32.h
** Used by FileSwitch.h to create the short synonyms for 32-bit file handles
**
** Tony van der Hoff 10-11-01
*/

#if !defined fileswch32_H
#define fileswch32_H

#if !defined osf32_H
#include "osf32.h"
#endif

#if !defined OSLIB_F8
#define bgetv bgetvw
#define bputv bputvw
#define os_bget os_bgetw
#define os_bput os_bputw
#define xbgetv xbgetvw
#define xbputv xbputvw
#define xos_bget xos_bgetw
#define xos_bput xos_bputw
#endif

#endif

/* End */
