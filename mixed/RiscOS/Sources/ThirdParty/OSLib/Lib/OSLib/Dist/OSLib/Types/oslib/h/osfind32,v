@/* osfind32.h
** Used by osfind.h to create the short synonyms for 32-bit file handles
**
** Tony van der Hoff 10-11-01
*/

#if !defined osfind32_H
#define osfind32_H

#if !defined osf32_H
#include "osf32.h"
#endif

#if !defined OSLIB_F8
#define osfind_close osfind_closew
#define osfind_openin osfind_openinw
#define osfind_openout osfind_openoutw
#define osfind_openup osfind_openupw
#define upcallfind_close upcallfind_closew
#define upcallfind_create_openup upcallfind_create_openupw
#define upcallfind_openup upcallfind_openupw
#define xosfind_close xosfind_closew
#define xosfind_openin xosfind_openinw
#define xosfind_openout xosfind_openoutw
#define xosfind_openup xosfind_openupw
#define xupcallfind_close xupcallfind_closew
#define xupcallfind_create_openup xupcallfind_create_openupw
#define xupcallfind_openup xupcallfind_openupw
#endif

#endif

/* End */
